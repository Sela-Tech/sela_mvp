{
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["eslint-plugin-prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "all",
        "singleQuote": true
      }
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
